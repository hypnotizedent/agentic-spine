#!/usr/bin/env bash
# orchestration-lane - Lane orchestration capability wrapper
# Called by: ops cap run orchestration.lane.{list,open,status}
set -euo pipefail

SPINE_REPO="${SPINE_REPO:-$HOME/code/agentic-spine}"
ACTION="${1:-list}"
shift || true

exec "$SPINE_REPO/ops/commands/lane.sh" "$ACTION" "$@"
