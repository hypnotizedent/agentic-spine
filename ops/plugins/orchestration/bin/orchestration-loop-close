#!/usr/bin/env bash
set -euo pipefail

usage() {
  cat <<'USAGE'
orchestration-loop-close - Close an orchestration loop when lanes are integrated

Usage:
  orchestration-loop-close --loop-id <id> [--force]
USAGE
}

case "${1:-}" in
  -h|--help)
    usage
    exit 0
    ;;
esac

echo "ERROR: loop close logic not wired yet"
exit 1
